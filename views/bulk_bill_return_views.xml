<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_bulk_bill_return_form" model="ir.ui.view">
            <field name="name">bulk.bill.return.form</field>
            <field name="model">bill.return.config</field>
            <field name="arch" type="xml">
                <form string="Bulk Bill Return" create="false" delete="false">
                <sheet>
                    <group>
                    <field name="vendor_ids"/>

                    <separator string="Storage Configuration" colspan="2"/>
                    <field name="location_ids"/>
                    <field name="default_location_id" required="1"/>

                    <separator string="Transit Configuration" colspan="2"/>
                    <field name="intermediate_location_id"/>
                    <field name="default_intermediate_location_id"/>

                    <separator string="Picking Settings" colspan="2"/>
                    <field name="picking_type_ids"/>
                    </group>
                </sheet>
                </form>
            </field>
        </record>





    <record id="action_bulk_bill_return_server" model="ir.actions.server">
        <field name="name">Bulk Bill Return</field>
        <field name="model_id" ref="model_bill_return_config"/>
        <field name="binding_model_id" ref="model_bill_return_config"/>
        <field name="state">code</field>
        <field name="code">action = env['bill.return.config']._get_action_bulk_bill_return()</field>
    </record>

</odoo>
